{
	"name": "nestjs-admin-prisma-panel",
	"version": "1.0.1",
	"description": "mapping prisma for admin db panel",
	"author": "Alexander Lvov",
	"license": "UNLICENSED",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
	},
	"devDependencies": {
		"@nestjs/common": "^10.0.0",
		"@prisma/client": "^6.1.0",
		"@types/node": "^20.3.1",
		"@typescript-eslint/eslint-plugin": "^8.0.0",
		"@typescript-eslint/parser": "^8.0.0",
		"eslint": "^8.0.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.0",
		"jest": "^29.5.0",
		"prettier": "^3.0.0",
		"typescript": "^5.1.3"
	},
	"peerDependencies": {
		"@nestjs/common": "^10.0.0",
		"@prisma/client": "^6.1.0"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	},
	"dependencies": {}
}
